{% extends 'base.html.twig' %}

{% block title %}Hello MatchsController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

            <li>Match : {{matchs.equipeDom}} - {{matchs.equipeExt}}</li>
            <li>Resultat : {{matchs.ScoreDom}} - {{matchs.ScoreExt}}</li>
            <li>Date : {{matchs.date|date('d/m/Y')}}</li>
            <li>Nom du Stade : {{stade.nomstade}}</li>
            <li>Localisation : {{stade.localisation}}</li>


    
</div>
{% endblock %}
